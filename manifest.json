{
  "background": {
    "scripts": [
      "./background/youtube.js",
      "./background/messagehandler.js",
      "./background/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "description": "Create Youtube Playlist from all linked videos in a page.",
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Youtube Playlist Generator",
  "oauth2": {
    "client_id": "",
    "scopes": [
      "https://www.googleapis.com/auth/youtube"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://www.youtube.com/",
    "identity"
  ],
  "version": "0.0.11"
}
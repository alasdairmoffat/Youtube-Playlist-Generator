{
  "background": {
    "scripts": [
      "./background/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "description": "Add all linked videos in a page to Youtube playlists.",
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  },
  "manifest_version": 2,
  "name": "Youtube Playlist Generator",
  "permissions": [
    "activeTab",
    "tabs",
    "https://www.youtube.com/"
  ],
  "version": "0.0.13"
}